  },
  "homepage": "https://github.com/March-mitsuki/white-logger#readme",
  "devDependencies": {
    "@types/luxon": "^3.1.0",
    "@types/node": "^18.11.13",
    "ts-loader": "^9.4.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@white-logger/shared": "latest",
    "luxon": "^3.1.1"
  }
